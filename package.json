{
  "name": "discord-attestation",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r --parallel run build",
    "dev": "pnpm -r --parallel run dev",
    "postinstall": "pnpm --filter \"{packages/contracts}\" compile",
    "lint": "pnpm -r --parallel run lint",
    "lint:fix": "prettier --write '**/*.sol' '**/*.ts' '**/*.json' '**/*.tsx' '**/*.json' '**/*.md'",
    "prepare": "husky",
    "test": "pnpm -r --parallel run test",
    "update-readme": "tsc && node dist/updateReadme.js"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2"
  },
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": ">=18.18.0",
    "pnpm": ">=9.10.0"
  },
  "pnpm": {
    "overrides": {
      "cookie@<0.7.0": ">=0.7.0",
      "cross-spawn@>=7.0.0 <7.0.5": ">=7.0.5",
      "elliptic@>=5.2.1 <=6.5.6": ">=6.5.7",
      "elliptic@>=4.0.0 <=6.5.6": ">=6.5.7",
      "elliptic@>=2.0.0 <=6.5.6": ">=6.5.7",
      "elliptic@<6.5.6": ">=6.5.6",
      "elliptic@<6.6.0": ">=6.6.0",
      "nanoid@<3.3.8": ">=3.3.8",
      "secp256k1@>=4.0.0 <4.0.4": ">=4.0.4",
      "secp256k1@=5.0.0": ">=5.0.1",
      "ws@>=7.0.0 <7.5.10": ">=7.5.10"
    }
  }
}
